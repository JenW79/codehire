{
  "name": "codehire",
  "version": "1.0.0",
  "description": "Codehire: A tech job search and tracker",
  "scripts": {
    "install": "npm install --prefix backend && npm install --prefix frontend",
    "dev:backend": "npm run --prefix backend start:development",
    "sequelize": "npm run --prefix backend sequelize",
    "sequelize-cli": "npm run --prefix backend sequelize-cli",
    "ensure-schema": "node backend/psql-setup-script.js",
    "backend-setup": "npm run --prefix backend migrate-and-seed",
    "build": "npm run --prefix frontend build",
    "start": "npm run --prefix backend start:production"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "react-toastify": "^11.0.5"
  }
}
